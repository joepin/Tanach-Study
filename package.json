{
  "name": "ts-react",
  "version": "1.0.0",
  "description": "A React-based client app for the Tanach Study Program",
  "main": "server.js",
  "scripts": {
    "clean": "rm -rf dist && rm -rf node_modules && rm -f package-lock.json",
    "build": "NODE_ENV=production webpack --mode production --progress --config webpack.config.js",
    "deploy": "npm run clean && npm i --only=dev && NODE_ENV=production webpack --mode production --progress && rm -rf node_modules/ && rm -f package-lock.json && npm i --only=prod",
    "start": "node server.js",
    "rebuild": "NODE_ENV=production webpack --mode production --progress --colors",
    "watch": "webpack --mode development --progress --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tanach-study/Tanach-Study.git"
  },
  "author": "Joey Pinhas",
  "license": "MIT",
  "engines": {
    "node": "8.0.0",
    "npm": "5.0.0"
  },
  "dependencies": {
    "connect-history-api-fallback": "^1.3.0",
    "dotenv": "^6.1.0",
    "express": "^4.15.3",
    "morgan": "^1.8.2",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-strict-mode": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "8.0.4",
    "babel-plugin-add-module-exports": "1.0.0",
    "classnames": "2.2.6",
    "css-loader": "1.0.0",
    "eslint": "5.7.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "exports-loader": "0.7.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "2.0.0",
    "html-webpack-plugin": "3.2.0",
    "html-webpack-template": "6.2.0",
    "imports-loader": "0.8.0",
    "postcss-loader": "3.0.0",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-ga": "2.5.3",
    "react-router-dom": "4.3.1",
    "style-loader": "0.23.1",
